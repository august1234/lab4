<struts>
	<package name="bookmanage" extends="struts-default">

		<action name="books" class="action.BookDetails" method="execute">
			<result name="success">/bookdetails.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="bookget" class="action.BookSelect" method="execute">
			<result name="success">/book.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="bookdel" class="action.BookDelete" method="execute">
			<result name="success">/delete.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
	</package>

</struts> 
